			var nameProduct = $(this).parent().parent().parent().find('.block2-name')();
			$(this).on('click', function(){
				swal(nameProduct, "is added to wishlist !", "success");
			});
		});

		$('.btn-addcart-product-detail').each(function(){
			var nameProduct = $('.product-detail-name')();
			$(this).on('click', function(){
				swal(nameProduct, "is added to wishlist !", "success");
			});
		});

		var handler = StripeCheckout.configure({
			key: 'pk_live_VJqGUWgq3JnJwbIbH8KT3Nz8',
			image: 'https://s3.amazonaws.com/the-trophyist/trophy.png',
			locale: 'auto',
			token: function(token, addresses) {
				var stripe_url = 'https://smlw9fh6jl.execute-api.us-east-1.amazonaws.com/prod/theTrophyist-createCharge';
				var data = {
					token: token,
					addresses: addresses,
					trophy: trophy.Name,
					engraving: selectEngraving || engraving.Name,
					note: $.trim($("#personal-note").val())					
				};
				var _data = JSON.stringify(data);

				// You can access the token ID with `token.id`.
				// Get the token ID to your server-side code for use.
				console.log(_data, addresses);

				$.post( stripe_url, _data, function( response ) {
          console.log(response);
        });
			}
		});

		document.getElementById('buy-button').addEventListener('click', function(e) {
			if (engraving || selectEngraving) {
				// Open Checkout with further options:
				handler.open({
					name: trophy.Name,
					description: selectEngraving || engraving.Name,
					amount: 2000,
					label: 'Product One',
					shippingAddress: true,
					// billingAddress: true,
					panelLabel: 'Pay'
				});
			} else {
				$('.engraving-selection').select2('open');
			}
			
			e.preventDefault();
		});

		// Close Checkout on page navigation:
		window.addEventListener('popstate', function() {
			handler.close();
		});
	</script>

