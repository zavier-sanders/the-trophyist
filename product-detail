		$(".selection-2").select2({
			minimumResultsForSearch: 20,
			dropdownParent: $('#dropDownSelect2')
		});

		$(".engraving-selection").on("select2:select", function (e) { 
			selectEngraving = $(e.currentTarget).select2('data')[0].text;
			engraving = undefined;
			console.log(selectEngraving);
		});

		var getUrlParameter = function getUrlParameter(sParam) {
				var sPageURL = decodeURIComponent(window.location.search.substring(1)),
						sURLVariables = sPageURL.split('&'),
						sParameterName,
						i;

				for (i = 0; i < sURLVariables.length; i++) {
						sParameterName = sURLVariables[i].split('=');

						if (sParameterName[0] === sParam) {
								return sParameterName[1] === undefined ? true : sParameterName[1];
						}
				}
		};

		var t =  parseInt(getUrlParameter('t'));
		var e = parseInt(getUrlParameter('e'));

		var data = JSON.parse(document.getElementById('product-data').innerHTML);

		var trophy = data.Trophies.find(x => x.ID === t);
		var engraving = data.Engravings.find(x => x.ID === e);
		var engravings = data.Engravings;
		var selectEngraving;

		$(".thumb-1").attr("src", trophy.Details[0]);
		$(".thumb-2").attr("src", trophy.Details[1]);
		$(".thumb-3").attr("src", trophy.Details[2]);
		
		$(".thumb-1").attr("data-thumb", trophy.Thumbnails[0]);
		$(".thumb-2").attr("data-thumb", trophy.Thumbnails[1]);
		$(".thumb-3").attr("data-thumb", trophy.Thumbnails[2]);

		$(".trophy-name").text(trophy.Name);

		$.each(engravings, function (i, item) {
				$('.engraving-selection').append($('<option>', { 
						value: item.ID,
						text : item.Name 
				}));
		});
		
		if(engraving) {
			$(".engraving-selection").val(engraving.ID);
		}

		console.log(trophy, engraving);
	</script>

