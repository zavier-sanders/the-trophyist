	<script type="text/javascript" src="vendor/sweetalert/sweetalert.min.js"></script>
	<script type="text/javascript">
		var getUrlParameter = function getUrlParameter(sParam) {
				var sPageURL = decodeURIComponent(window.location.search.substring(1)),
						sURLVariables = sPageURL.split('&'),
						sParameterName,
						i;

				for (i = 0; i < sURLVariables.length; i++) {
						sParameterName = sURLVariables[i].split('=');

						if (sParameterName[0] === sParam) {
								return sParameterName[1] === undefined ? true : sParameterName[1];
						}
				}
		};

		var e = parseInt(getUrlParameter('e'));


		$('.block2-btn-addcart').each(function(){
			var nameProduct = $(this).parent().parent().parent().find('.block2-name').attr("data-value");
			console.log(nameProduct);
			$(this).on('click', function(){
				if (e) {
					window.location.href = "product-detail?t=" + nameProduct + "&e=" + e;	
				} else {
					window.location.href = "product-detail?t=" + nameProduct;
				}
			});
		});

		$('.block2-btn-addwishlist').each(function(){
			var nameProduct = $(this).parent().parent().parent().find('.block2-name')();
			$(this).on('click', function(){
				swal(nameProduct, "is added to wishlist !", "success");
			});
		});
	</script>

